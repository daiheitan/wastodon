{
    "manifest_version": 2,
    "name": "Wastodon",
    "version": "1.0",
    "description": "Reblog a weibo to Mastodon",
    "icons": {
        "48": "icons/border-48.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.weibo.com/*"
            ],
            "js": [
                "wastodon.js"
            ]
        }
    ]
}